<!DOCTYPE html>
<html>
<title>Edupro</title>
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
 <link rel="stylesheet" href="/style.css">
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
      <script src="https://code.jquery.com/jquery-3.3.1.js"></script> <!----------    media Slide down ---->
	  <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
           <link rel="stylesheet" href="/style4.css">
             <link rel="icon" href="/Users/User/Desktop/Edupro/logor.png">
             <link href="https://fonts.googleapis.com/css?family=Righteous&display=swap" rel="stylesheet">
      <link rel="stylesheet" href="/style5.css"> 
       <link rel="stylesheet" href="/style7.css"> 
</head>
<style>
.mySlides {display:none}
.w3-left, .w3-right, .w3-badge {cursor:pointer}
.w3-badge {height:13px;width:13px;padding:0}
</style>


<body>
<!-- Navbar (sit on top) -->
<div class="topnav" id="myTopnav">
                <a href="/auth/home">Home</a>
       
          <a href="/auth/categories">Categories</a>
       
          <a href="/auth/news">News</a>
      
          <a href="/auth/About">About</a>
       
        
          <a href="/auth/contact">Contact</a>
           <a href="/auth/logout">Logout</a>
       
          <a href="/auth/profile">Profile</a>
         
          
  <a href="javascript:void(0);" class="icon" onclick="myFunction()">
    <i class="fa fa-bars"></i>
  </a>
</div>


{{#if message}}
	<h3 style="color: red;">{{message}}</h3>
{{/if}}

{{#each results}}


<div class="left-box" >
  <div class="top-left">
  <img  src="https://w7.pngwing.com/pngs/527/663/png-transparent-logo-person-user-person-icon-rectangle-photography-computer-wallpaper.png" alt="Cody Man">
  <div class="name">{{this.StudentLastName}}
                      {{this.StudentFirstName}}</div>
    
    <div class="phone">
      <p> Tel</p>
      <img   class="objective-img" src="http://icons.iconarchive.com/icons/icons8/android/512/Mobile-Phone-2-icon.png">
      <div class="phone-text">  {{this.StudentPhone}}<br><br>
      </div>
    </div>
    
     <div class="phone">
      <p> Email</p>
      <img   class="objective-img" src="http://static.wixstatic.com/media/d822a6_af29afe70e1a4e9891f94a627914b6ec.png">
      <div class="phone-text">  {{this.StudentEmail}}<br><br>
      </div>
    </div>
      <div class="phone">
        <p>Location</p>
      <img   class="objective-img" src="http://icons.iconarchive.com/icons/icons8/android/512/Mobile-Phone-2-icon.png">
      <div class="phone-text"> {{this.StudentAddress}}, {{this.StudentLocation}}<br></div>
      
     </div>
</div>
</div>
 
{{/each}}


  

</div>



<!-- Header -->
<script>
function myFunction() {
  var x = document.getElementById("myTopnav");
  if (x.className === "topnav") {
    x.className += " responsive";
  } else {
    x.className = "topnav";
  }
}
</script>


</body>
</html>