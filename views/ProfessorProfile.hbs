<!-- Header -->
<script>
function myFunction() {
  var x = document.getElementById("myTopnav");
  if (x.className === "topnav") {
    x.className += " responsive";
  } else {
    x.className = "topnav";
  }
}
</script>
<script>
  ;(function($) {
 var $app = $('#app');

 var notifications = {
  init: function() {
   $app.on('notify', function(e) {

    // This is just an example for notification alerts...
    $('<div class="alert alert-success">' + e.message + '</div>')
     .prependTo('#app')
     .hide()
     .slideDown()
     .delay(2000)
     .slideUp(function() {
      $(this).remove();
     });
   });
  }
 };

 var sampleModule = {
  init: function() {

   $('form').on('enter', function() {
    var txt = $('#message').val() || 'Please enter some Text';

    // Trigger a Notification
    $app.trigger({
     type: 'notify',
     message: txt
    });
    
    return false;
   });
  }
 };

 notifications.init();
 sampleModule.init();
})(jQuery);
</script>

